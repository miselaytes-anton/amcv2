{
  "root": true,
  "extends": ["warp/es6", "warp/jsx"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "eslint-plugin-react"
  ],
  "rules": {
    "no-sync": 0,
    "max-len": 0,
    "react/prop-types": 0
  }
}
